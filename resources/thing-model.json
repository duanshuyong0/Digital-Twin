{
    "thingId": "org.eclipse.ditto.example:raspberry",
    "acl": {
        "ditto": {
            "READ": true,
            "WRITE": true,
            "ADMINISTRATE": true
        },
        "raspberry": {
            "READ": true,
            "WRITE": true,
            "ADMINISTRATE": false
        }
    },
    "attributes": {
        "manufacturer": "Raspberry Pi Foundation",
        "hostname": "Raspditto"
    },
    "features": {
        "TemperatureSensor": {
            "properties": {
                "temperatureValue": 25.0,
                "lastUpdate": "2019-01-01 00:00:00.100000",
                "samplingRate": 1
            }
        },
      	"FanActuator": {
      	    "properties": {
      	        "fanState": 0,
      	        "lastUpdate": "2019-01-01 00:00:00.100000"
      	    }
      	}
    }
}
